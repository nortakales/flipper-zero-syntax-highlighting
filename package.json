{
  "name": "flipper-zero-syntax-highlighting",
  "displayName": "Flipper Zero: Syntax Highlighting",
  "description": "Syntax highlighting for Flipper Zero files",
  "publisher": "nortakales",
  "version": "1.3.0",
  "engines": {
    "vscode": "^1.86.0"
  },
  "keywords": [
    "flipper zero",
    "nfc",
    "subghz",
    "rfid",
    "infrared",
    "picopass",
    "xr",
    "cross remote",
    "bad usb",
    "ducky script",
    "flipper music file",
    "fmf",
    "picopass",
    "u2f",
    "ibtn"
  ],
  "categories": [
    "Programming Languages"
  ],
  "homepage": "https://github.com/nortakales/flipper-zero-syntax-highlighting",
  "repository": {
    "type": "git",
    "url": "https://github.com/nortakales/flipper-zero-syntax-highlighting.git"
  },
  "sponsor": {
    "url": "https://www.buymeacoffee.com/nortakales"
  },
  "bugs": {
    "url": "https://github.com/nortakales/flipper-zero-syntax-highlighting/issues"
  },
  "icon": "resources/icon.png",
  "contributes": {
    "languages": [
      {
        "id": "flipper-ir",
        "aliases": [
          "Flipper Zero - Infrared"
        ],
        "extensions": [
          ".ir"
        ],
        "configuration": "./language-configuration/ir.json"
      },
      {
        "id": "flipper-sub",
        "aliases": [
          "Flipper Zero - SubGhz"
        ],
        "extensions": [
          ".sub"
        ],
        "configuration": "./language-configuration/sub.json"
      },
      {
        "id": "flipper-nfc",
        "aliases": [
          "Flipper Zero - NFC"
        ],
        "extensions": [
          ".nfc"
        ],
        "configuration": "./language-configuration/nfc.json"
      },
      {
        "id": "flipper-rfid",
        "aliases": [
          "Flipper Zero - RFID"
        ],
        "extensions": [
          ".rfid"
        ],
        "configuration": "./language-configuration/rfid.json"
      },
      {
        "id": "flipper-ibtn",
        "aliases": [
          "Flipper Zero - iButton"
        ],
        "extensions": [
          ".ibtn"
        ],
        "configuration": "./language-configuration/ibtn.json"
      },
      {
        "id": "flipper-u2f",
        "aliases": [
          "Flipper Zero - U2F"
        ],
        "extensions": [
          ".u2f"
        ],
        "configuration": "./language-configuration/u2f.json"
      },
      {
        "id": "flipper-bad-usb",
        "aliases": [
          "Flipper Zero - Bad USB"
        ],
        "extensions": [
          ".txt"
        ],
        "configuration": "./language-configuration/bad-usb.json"
      },
      {
        "id": "flipper-picopass",
        "aliases": [
          "Flipper Zero - Picopass"
        ],
        "extensions": [
          ".picopass"
        ],
        "configuration": "./language-configuration/picopass.json"
      },
      {
        "id": "flipper-cross-remote",
        "aliases": [
          "Flipper Zero - Cross Remote",
          "Flipper Zero - XR"
        ],
        "extensions": [
          ".xr"
        ],
        "configuration": "./language-configuration/xr.json"
      },
      {
        "id": "flipper-music-file",
        "aliases": [
          "Flipper Zero - Music File"
        ],
        "extensions": [
          ".fmf"
        ],
        "configuration": "./language-configuration/fmf.json"
      },
      {
        "id": "flipper-application-manifest",
        "aliases": [
          "Flipper Zero - Application Manifest"
        ],
        "extensions": [
          ".fam"
        ],
        "configuration": "./language-configuration/fam.json"
      }
    ],
    "grammars": [
      {
        "language": "flipper-ir",
        "scopeName": "source.flipper.ir",
        "path": "./syntaxes/ir.tmLanguage.json"
      },
      {
        "language": "flipper-sub",
        "scopeName": "source.flipper.sub",
        "path": "./syntaxes/sub.tmLanguage.json"
      },
      {
        "language": "flipper-nfc",
        "scopeName": "source.flipper.nfc",
        "path": "./syntaxes/nfc.tmLanguage.json"
      },
      {
        "language": "flipper-rfid",
        "scopeName": "source.flipper.rfid",
        "path": "./syntaxes/rfid.tmLanguage.json"
      },
      {
        "language": "flipper-ibtn",
        "scopeName": "source.flipper.ibtn",
        "path": "./syntaxes/ibtn.tmLanguage.json"
      },
      {
        "language": "flipper-u2f",
        "scopeName": "source.flipper.u2f",
        "path": "./syntaxes/u2f.tmLanguage.json"
      },
      {
        "language": "flipper-bad-usb",
        "scopeName": "source.flipper.bad-usb",
        "path": "./syntaxes/bad-usb.tmLanguage.json"
      },
      {
        "language": "flipper-picopass",
        "scopeName": "source.flipper.picopass",
        "path": "./syntaxes/picopass.tmLanguage.json"
      },
      {
        "language": "flipper-cross-remote",
        "scopeName": "source.flipper.xr",
        "path": "./syntaxes/xr.tmLanguage.json"
      },
      {
        "language": "flipper-music-file",
        "scopeName": "source.flipper.fmf",
        "path": "./syntaxes/fmf.tmLanguage.json"
      },
      {
        "language": "flipper-application-manifest",
        "scopeName": "source.flipper.fam",
        "path": "./syntaxes/fam.tmLanguage.json"
      }
    ]
  }
}